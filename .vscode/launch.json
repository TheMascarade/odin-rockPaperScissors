		"type": "node"
	},
		{
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"name": "nodemon",
			"program": "${workspaceFolder}/JS/rockPaperScissors.js",
			"request": "launch",
			"restart": true,
			"runtimeExecutable": "nodemon",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Launch index.html",
			"file": "${workspaceFolder}/index.html"
		},

	]
}